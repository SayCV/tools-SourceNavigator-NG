#!/bin/sh

prefix="@prefix@"
exec_prefix="@exec_prefix@"
bindir="@bindir@"
datarootdir="@datarootdir@"
datadir="@datadir@"
etcdir="$datadir/etc"

if test "$SN_HOME" = ""; then
   SN_HOME="$prefix"
   export SN_HOME
fi

TCL_LIBRARY=$SN_HOME/share/tcl8.1
export TCL_LIBRARY

exec "$bindir/hyper" -file "$etcdir/snscenario" "--" -home "$prefix" ${1+"$@"}
